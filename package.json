{
    "name": "naverpay",
    "version": "0.0.0",
    "private": true,
    "description": "Naver Pay frontend package ",
    "type": "module",
    "scripts": {
        "build": "tsc -b && vite build",
        "lint": "eslint .",
        "lint:check": "eslint . --max-warnings 0",
        "prettier": "prettier . --write",
        "prettier:check": "prettier . --check",
        "format": "npm run prettier && npm run lint",
        "check": "npm run prettier:check && npm run lint:check",
        "start": "vite"
    },
    "dependencies": {
        "@reduxjs/toolkit": "^2.8.2",
        "axios": "^1.10.0",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-redux": "^9.2.0",
        "react-router-dom": "^7.7.0",
        "styled-components": "^6.1.19"
    },
    "devDependencies": {
        "@naverpay/eslint-config": "^2.3.0",
        "@naverpay/prettier-config": "^1.0.1",
        "@types/node": "^22.16.4",
        "@types/react": "^18.3.23",
        "@types/react-dom": "^18.3.7",
        "@types/react-router-dom": "^5.3.3",
        "@vitejs/plugin-react": "^4.6.0",
        "msw": "^2.10.4",
        "typescript": "~5.8.3",
        "vite": "^7.0.4"
    },
    "msw": {
        "workerDirectory": [
            "bublic"
        ]
    }
}
