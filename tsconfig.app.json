{
    "compilerOptions": {
        "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
        "target": "ES2022",
        "module": "ES2022",
        "skipLibCheck": true,
        "noImplicitAny": true,
        "moduleResolution": "bundler",
        "allowImportingTsExtensions": true,
        "verbatimModuleSyntax": true,
        "noEmit": true,
        "jsx": "react-jsx"
    },
    "include": ["src"]
}
